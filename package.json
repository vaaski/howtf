    "stub": "unbuild --stub",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.16.3",
    "@types/prompts": "^2.4.4",
